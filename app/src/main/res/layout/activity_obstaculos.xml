<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.estudiante.citybiker.obstaculos"
    android:background="@mipmap/obstaculos800x566">


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton5"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:onClick="jugabilidad"
        android:layout_marginTop="47dp"
        android:background="@drawable/boton1" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton6"
        android:layout_below="@+id/imageButton5"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="29dp"
        android:background="@drawable/boton2" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton7"
        android:layout_below="@+id/imageButton6"
        android:layout_alignParentEnd="true"
        android:onClick="puntajes"
        android:layout_marginTop="32dp"
        android:background="@drawable/boton3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton8"
        android:layout_below="@+id/imageButton7"
        android:layout_alignParentEnd="true"
        android:onClick="atras"
        android:layout_marginTop="32dp"
        android:background="@drawable/atras" />
</RelativeLayout>
